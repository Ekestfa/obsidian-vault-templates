views:
  - type: table
    name: Table
    filters:
      and:
        - '!file.name.contains("Template")'
        - file.tags.contains("project")
    order:
      - progress
      - file.name
      - file.ctime
      - remark
      - release
    sort: []
    columnSize:
      formula.Untitled: 122
